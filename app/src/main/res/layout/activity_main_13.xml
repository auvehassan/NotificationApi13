<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.auvehassan.android._13.MainActivity13">


    <Button
        android:id="@+id/buttonNotification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notification"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_margin="8dp"/>

    <Button
        android:id="@+id/buttonCopyPaste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Copy Paste"
        app:layout_constraintStart_toEndOf="@id/buttonNotification"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_margin="8dp"/>
    <Button
        android:id="@+id/buttonPhotoPicker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Photo Picker"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonNotification"
        android:layout_margin="8dp"/>

    <Button
        android:id="@+id/buttonRevokePermission"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Revoke Permission"
        app:layout_constraintStart_toEndOf="@id/buttonPhotoPicker"
        app:layout_constraintTop_toBottomOf="@+id/buttonCopyPaste"
        android:layout_margin="8dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>